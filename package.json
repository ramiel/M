{
  "name": "memoizy",
  "version": "1.0.0",
  "description": "Powerful memoization helper",
  "repository": "github:ramiel/memoizy",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-only -- --coverage",
    "test-only": "NODE_ENV=test jest",
    "test-watch": "npm run test-only -- --watch"
  },
  "keywords": [
    "memoize",
    "cache"
  ],
  "author": {
    "name": "Fabrizio Ruggeri",
    "email": "fabrizio.ruggeri@gmail.com",
    "url": "http://www.ramielcreations.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "jest": "^23.6.0"
  },
  "dependencies": {},
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "lcov",
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!.*.js"
    ]
  }
}
